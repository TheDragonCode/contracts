{
    "name": "andrey-helldar/contracts",
    "description": "The Andrey Helldar Contracts package",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "contracts",
        "interfaces"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "source": "https://github.com/andrey-helldar/contracts"
    },
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "psr/http-message": "^1.0.1",
        "symfony/http-client": "^4.0|^5.0",
        "symfony/polyfill-php80": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\Contracts\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
